        <action>Permanent</action>
        <isActive>true</isActive>
        <source>/docs</source>
        <target>/apex/Documentation</target>
    </siteRedirectMappings>
    <siteType>Visualforce</siteType>
    <subdomain>x3st-surveys-developer-edition</subdomain>
